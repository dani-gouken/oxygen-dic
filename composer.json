{
    "name": "atom/dic",
    "description": "A simple and expressive Dependency injection container for PHP",
    "type": "library",
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^9.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nghokeng Daniel",
            "email": "nghokend@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit --coverage-clover=coverage.xml"
    },
    "minimum-stability": "beta",
    "require": {
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Atom\\DI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atom\\DI\\Test\\": "tests"
        },
        "files": [
            "tests/Misc/functions.php"
        ]
    }
}
