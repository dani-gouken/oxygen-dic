{
    "name": "daniel/expressive-dic",
    "description": "A simple and expressive Dependency injection container for PHP",
    "type": "library",
    "require-dev": {
        "kahlan/kahlan": "^4.6",
        "squizlabs/php_codesniffer": "3.*",
        "victorjonsson/markdowndocs": "^1.3",
        "phpunit/phpunit": "^9.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "globexcam.dani",
            "email": "globexcam.dani@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "beta",
    "require": {
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\DI\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Oxygen\\DI\\Test\\": "tests"
        },
        "files": [
            "tests/Misc/functions.php"
        ]
    }
}